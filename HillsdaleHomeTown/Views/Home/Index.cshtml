<section id="register">
<div class="container-fluid register text-center bg-resgister">
    <div class="row left-align">
        <div class="col-lg-6 col-md-12 col-sm-12 row ">
            <div class="col-1"></div>
            <div class="col-10 bg-white p-5" style="border-radius: 20px;">
                 <form method="get" asp-action="Index" asp-controller="Home" id="regiter">
                     <h2 class="heading text-center">Register For <span class="sub-heading"><strong>Inquiry</strong></span></h2>
                      <div class="form-group mt-4">
                        <label class="mb-2" for="Firstname">First Name</label>
                        <input type="text" class="form-control" id="firstname" placeholder="jack">
                      </div>
                      <div class="form-group mt-3">
                        <label class="mb-2" for="Lastname">Last Name</label>
                        <input type="text" class="form-control" id="lastname" placeholder="sparrow">
                      </div>
                      <div class="form-group mt-3">
                        <label class="mb-2" for="email">Email</label>
                        <input type="email" class="form-control" id="email" placeholder="jack@example.com">
                      </div>
                      <div class="form-group mt-3">
                        <label class="mb-2" for="phone">Phone</label>
                        <input type="tel" class="form-control" id="phone" placeholder="(431) 8854-697">
                      </div>
                      <div class="form-group mt-3">
                        <label class="mb-2" for="phone">Phone</label>
                        <input type="tel" class="form-control" id="phone" placeholder="(431) 8854-697">
                      </div>
                      <div class="form-group mt-3 mb-3">
                        <label class="mb-2" for="broker">Are you a Broker</label>
                        <select class="form-control" id="broker">
                          <option>Yes</option>
                          <option>No</option>
                        </select>
                      </div>
                      <div class="form-group mt-2">
                          <span class="text-black text-lowercase">BY CHECKING THE BOX, YOU AGREE TO RECEIVE COMMUNICATIONS FROM HOMETOWN HILLSDALE*</span>
                        <div>
                            <label>
                                <input type="checkbox" name="agree" value="I AGREE">I AGREE
                                </label>
                        </div>
                      </div>
                      <button class="btn bg-theme mt-3" type="submit">Send</button>

                 </form>
            </div>
           <div class="col-1"></div>
        </div>
        <div class="col-lg-6 col-md-12 col-sm-12 content-align" >
            <div class="text-center">      
                <img class="image-width mt-5" src="~/img/hometown-hillsdale-white.png">
            </div>
